alias tnew="tmux new -s"
alias rl="source ~/.zshrc"
alias vim="nvim"
alias vi="nvim"
alias lg="lazygit"
alias mypc="ssh wherever230@192.168.1.6"
#alias walk="walk --icons"
system=$(uname -s)
if [[ "$system" = "Linux" ]];then
    alias cat="batcat"
    alias pbcopy='xclip -selection clipboard'
elif [[ "$system" = "Darwin" ]];then
    alias cat="bat"
fi

